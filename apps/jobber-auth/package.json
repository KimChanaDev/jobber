{
  "name": "@jobber/jobber-auth",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "--node-env=production"
          ],
          "cwd": "apps/jobber-auth"
        },
        "configurations": {
          "development": {
            "args": [
              "--node-env=development"
            ]
          }
        },
        "dependsOn": [
          "generate-types"
        ]
      },
      "prune-lockfile": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "executor": "@nx/js:prune-lockfile",
        "outputs": [
          "{workspaceRoot}/apps/jobber-auth/dist/package.json",
          "{workspaceRoot}/apps/jobber-auth/dist/package-lock.json"
        ],
        "options": {
          "buildTarget": "build"
        }
      },
      "copy-workspace-modules": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "outputs": [
          "{workspaceRoot}/apps/jobber-auth/dist/workspace_modules"
        ],
        "executor": "@nx/js:copy-workspace-modules",
        "options": {
          "buildTarget": "build"
        }
      },
      "prune": {
        "dependsOn": [
          "prune-lockfile",
          "copy-workspace-modules"
        ],
        "executor": "nx:noop"
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@jobber/jobber-auth:build",
          "runBuildTargetDependencies": true
        },
        "configurations": {
          "development": {
            "buildTarget": "@jobber/jobber-auth:build:development"
          },
          "production": {
            "buildTarget": "@jobber/jobber-auth:build:production"
          }
        }
      },
      "generate-types": {
        "command": "prisma generate",
        "options": {
          "cwd": "apps/jobber-auth"
        }
      },
      "migrate-prisma": {
        "command": "prisma migrate dev --name",
        "_comment": "example use: nx migrate-prisma -- user_table",
        "options": {
          "cwd": "apps/jobber-auth"
        }
      },
      "test": {
        "dependsOn": [
          "generate-types"
        ]
      },
      "docker:build": {
        "dependsOn": [
          "build",
          "prune"
        ]
      }
    }
  },
  "dependencies": {}
}
